{
  "name": "seiken-fx",
  "version": "0.2.1",
  "type": "module",
  "description": "A lightweight functional programming utility library for TypeScript/JavaScript",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && rollup -c",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prepublishOnly": "npm run test && npm run lint && npm run build"
  },
  "keywords": [
    "functional",
    "fp",
    "typescript",
    "javascript",
    "functional-programming",
    "utils",
    "utilities",
    "result",
    "either",
    "option",
    "monads",
    "immutable",
    "composition",
    "pipe",
    "compose",
    "curry"
  ],
  "author": "Gabriel Seixas",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/camisetags/seiken-fx.git"
  },
  "bugs": {
    "url": "https://github.com/camisetags/seiken-fx/issues"
  },
  "homepage": "https://github.com/camisetags/seiken-fx#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.5.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.13.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.8.1",
    "typescript": "^4.9.5"
  }
}
